<CustomeMDExpansionChevronRight>:
    icon: "chevron-right"
    disabled: True
    md_bg_color_disabled: 0, 0, 0, 0

    canvas.before:
        PushMatrix
        Rotate:
            angle: self._angle
            axis: (0, 0, 1)
            origin: self.center
    canvas.after:
        PopMatrix


